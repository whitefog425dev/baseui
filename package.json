    "styletron-react": "^4.3.0",
    "react": ">= 16.4.0 < 17",
    "react-dom": ">= 16.4.0 < 17"
  },
  "engines": {
    "node": ">=8.11.0",
    "npm": ">=5.6.0",
    "yarn": ">=1.5.1"
  }
