{
  "name": "baseui",
  "version": "0.0.1",
  "description": "React implementation of uber design components platform.",
  "keywords": ["react", "uber", "base-ui"],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/uber-web/baseui/issues"
  },
  "homepage": "https://github.com/uber-web/baseui#readme",
  "main": "dist/index.js",
  "jsnext:main": "dist/index.es.js",
  "module": "dist/index.es.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uber-web/baseui.git"
  },
  "scripts": {
    "lint": "eslint ./",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "test": "yarn lint && yarn unit-test",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o docs",
    "precommit": "pretty-quick --staged && yarn run test",
    "build": "rollup -c rollup.config.js",
    "prepublish": "npm run build",
    "unit-test": "yarn jest --coverage",
    "build-e2e": "rollup -c rollup.config_e2e.js",
    "serve-e2e": "node e2e/serve.js",
    "test-e2e":
      "pkill \"chromedriver|geckodriver|safaridriver\" | jest --config=jest-e2e.config.js"
  },
  "devDependencies": {
    "@commitlint/cli": "^6.1.3",
    "@commitlint/config-conventional": "^6.1.3",
